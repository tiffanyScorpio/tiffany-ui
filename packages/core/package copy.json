{
	"name": "tiffany-ui",
	"version": "1.0.0",
	"description": "components library by Vue3 + Ts + element plus",
	"type": "module",
	"main": "./dist/umd/index.umd.cjs",
	"module": "./dist/es/index.js",
	"types": "./dist/types/core/index.d.ts",
	"files": ["dist"],
	"exports": {
		".": {
			"import": "./dist/es/index.js",
			"require": "./dist/umd/index.umd.cjs",
			"types": "./dist/types/core/index.d.ts"
		},
		"./dist/": {
			"import": "./dist/",
			"require": "./dist/"
		}
	},
	"sideEffects": ["./dist/index.css"],
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"build": "pnpm run build-only && pnpm run build-umd",
		"build-only": "pnpm run build-es && pnpm run move-style",
		"build-umd": "vite build --config vite.umd.config.ts",
		"build-es": "vite build --config vite.es.config.ts",
		"move-style": "move-file dist/es/index.css dist/index.css"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@element-plus/icons-vue": "^2.3.1",
		"@popperjs/core": "^2.11.8",
		"async-validator": "^4.2.5",
		"element-plus": "^2.9.2"
	},
	"devDependencies": {
		"@tiffany-ui/components": "workspace:*"
	},
	"peerDependencies": {
		"vue": "^3.5.13"
	}
}
